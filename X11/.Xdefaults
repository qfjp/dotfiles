!Cursor
Xcursor.theme: Vanilla-DMZ
Xcursor.size: 12

!Xterm
XTerm*backarrowKey:       false
XTerm*termName:           xterm-256color
XTerm*faceName:           xft:PragmataPro:size=9
XTerm*metaSendsEscape:    true
XTerm*eightBitInput:      false
XTerm*cursorUnderLine:    true
XTerm*highlightSelection: true

!URxvt
URxvt.font:            xft:PragmataPro:size=9:antialias=true
URxvt.cursorUnderline: True
URxvt.scrollBar:       False
URxvt.saveLines:       0
URxvt.internalBorder:  0
URxvt.externalBorder:  0

!Disable URxvt perl extensions
!URxvt.perl-ext:
!URxvt.perl-ext-common:

!Yankable URLS
! Alt+U = Enter Selction Mode
! J     = Select next downward URL
! K     = Select next upward URL
! Enter = Open selected URL in browser and quit selection mode
! O     = Open selected URL in browser
! Y     = Yank URL and quit selection mode
! ESC   = quit selection mode
URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search
URxvt.url-select.launcher:  firefox
URxvt.url-select.underline: true
URxvt.clipboard.copycmd:    xsel -i
URxvt.clipboard.pastecmd:   xsel -o

!URxvt.letterSpace:     -1
!URxvt.fading:          100
!URxvt.pastableTabs:    on
!URxvt.colorUL:         Red
Emacs.font: Fantasque Sans Mono-10

Vim.font: Fantasque Sans Mono-10

